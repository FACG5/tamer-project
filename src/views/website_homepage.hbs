<section class="main__head">
  <img class="main_img" src="/img/l.png">
  <h1 class="main__h1">نحن مجتمع فلسطينى تعلمّي حر آمن</h1>
</section>
<section class="main__elem">
  <section class="main__search">
    <h1 class="search__h1">ابحث فى المكتبة</h1>
    <form>
      <input type="text" placeholder="بحث" name="search" id="input-search" class="input--search">
      <input type="submit" class="submit" id="submit-search" value="بحث">
    </form>
    <div class="main__search--book" id="search"></div>
  </section>
  <section>
    <h1 class="search__h1">أكثر الكتب استعارة</h1>
    <div class="main__elem--book">
      {{#each res}}
        <div id="{{bookLibraryId}}">
          <img src="{{imageUrl}}">
          <h5>{{name}}</h5>
        </div>
      {{/each}}
    </div>
  </section>
</section>
